{
  "name": "desktop-viewer",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.3",
    "@testing-library/user-event": "^12.6.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.1",
    "web-vitals": "^0.2.4",
    "wait-on": "5.2.1",
    "react-router-dom": "^5.2.0",
    "react-bootstrap": "^1.4.3",
    "react-select": "^3.2.0",
    "materialize-css": "^1.0.0",
    "electron-is-dev": "^1.2.0"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "electron": "^11.2.0",
    "electron-packager": "^15.2.0",
    "electron-pack": "^1.3.2",
    "electron-prebuilt-compile": "8.2.0",
    "electron-builder": "^22.9.1",
    "nodemon": "^2.0.7"
  },
  "scripts": {
    "start": "set PORT=8080 && react-scripts start",
    "build": "react-scripts build",
    "electron-pack": "npm run build && electron-builder -c.extraMetadata.main=build/electron.js",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "watch": "nodemon --exec electron .",
    "make": "electron-forge make",
    "dev": "concurrently \"npm run start\" \"wait-on http://localhost:3006 && npm run watch\""
  },
  "build": {
    "appId": "com.example.electron-react",
    "productName": "Расписание",
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "dist/**/**",
      "package.json"
    ],
    "directories" : {
      "buildResources": "assets"
    }
  },
  "main": "public/electron.js",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
